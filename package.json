{
  "name": "mechine",
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "Gnak",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:4000/"
    }
  },
  "build": {
    "appId": "com.mechine.app",
    "productName": "Mechine",
    "files": [
      "./dist",
      "./dist-electron"
    ],
    "extraFiles": [
      {
        "from": "./config/",
        "to": "./config/"
      }
    ],
    "win": {
      "icon": "dist/logo.png"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "runAfterFinish": true,
      "installerIcon": "dist/favicon.ico",
      "uninstallerIcon": "dist/favicon.ico",
      "installerHeaderIcon": "dist/favicon.ico",
      "shortcutName": "WTF",
      "warningsAsErrors": false
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "pkg": {
      "isRelocatable": false,
      "overwriteAction": "upgrade"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.52",
    "@unocss/preset-attributify": "^0.53.4",
    "@unocss/reset": "^0.53.4",
    "@unocss/transformer-directives": "^0.53.4",
    "@unocss/transformer-variant-group": "^0.55.4",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "electron": "^24.1.3",
    "electron-builder": "^23.6.0",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "unocss": "^0.53.4",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vue": "^3.2.47",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.2.0",
    "vuestic-ui": "^1.7.0"
  },
  "dependencies": {}
}
